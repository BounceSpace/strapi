{
  "kind": "collectionType",
  "collectionName": "locations",
  "info": {
    "singularName": "location",
    "pluralName": "locations",
    "displayName": "Location",
    "description": "Locations"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subtitle": {
      "type": "string"
    },
    "about": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "locationTags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::locationtag.locationtag"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images", "files", "videos"]
    },
    "locationGallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images", "files", "videos"]
    },
    "locationVideo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images", "files", "videos"]
    },
    "meetingSpacesDescriptionText": {
      "type": "text"
    },
    "meetingSpacesHeaderText": {
      "type": "string"
    },
    "spaces": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::space.space"
    },
    "locationOptions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::locationoption.locationoption"
    },
    "optionsHeaderText": {
      "type": "string"
    },
    "optionsDescriptionText": {
      "type": "text"
    },
    "plansButtonText": {
      "type": "string"
    },
    "plansButtonDestinationUrl": {
      "type": "string",
      "regex": "^(https?:\\/\\/)?[\\w.-]+(\\.[\\w.-]+)+([\\/\\w .-]*)*\\/?$"
    },
    "vatToggleText": {
      "type": "string"
    },
    "bookMeetingSpaceUrl": {
      "type": "string"
    }
  }
}

