{
  "kind": "collectionType",
  "collectionName": "spaces",
  "info": {
    "singularName": "space",
    "pluralName": "spaces",
    "displayName": "Space",
    "description": "Spaces available at locations"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "text"
    },
    "spaceTags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::spacetag.spacetag"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images", "files", "videos"]
    },
    "optixResourceId": {
      "type": "string"
    }
  }
}

